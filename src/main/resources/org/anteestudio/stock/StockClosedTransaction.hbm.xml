<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.anteestudio.stock.hbm">

    <class name="StockClosedTransaction" table="stock_closed_transaction">
        <id name="trans_id" column="trans_id">
            <generator class="increment"/>
        </id>
	<property name="symbol" />
	<property name="buy_price" />
	<property name="buy_date" />
	<property name="sell_price" />
	<property name="sell_date" />
	<property name="quantity" />
        <property name="date" type="timestamp" column="EVENT_DATE"/>
    </class>

</hibernate-mapping>
